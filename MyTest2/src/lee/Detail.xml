<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="board2">
 
 <!-- 상품 전체에 대한 리스트 불러오기 -->
  <select id="prd_list" resultType="board2">
    select * from prd
 </select>
 
 <!-- 상품 Sf에 대한 리스트 불러오기 -->
 <select id="Sf" parameterType="String" resultType="board2">
    select * from prd where prd_id=#{Sf_prd_id}
 </select>
 
  <!-- 상품 Sf에 대한 리스트 불러오기 -->
 <select id="Mf" parameterType="String" resultType="board2">
    select * from prd where prd_id=#{Mf_prd_id}
 </select>
 
  <!-- 상품 Sf에 대한 리스트 불러오기 -->
 <select id="Lf" parameterType="String" resultType="board2">
    select * from prd where prd_id=#{Lf_prd_id}
 </select>
 
</mapper>







